{
  "compilerOptions": {
    "outDir": "dist",
    "target": "ES2015",
    "moduleResolution": "node",
    "strict": true,
    "allowJs": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "lib": [
      "ES2020"
    ],
    "noImplicitAny": false, // 是否在表达式和声明上有隐含的any类型时报错
  }
}
